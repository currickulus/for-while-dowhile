public class timestable {    
    
    public static void main (String[] args) {
        for(int i=1;i<=8;i++)
        {   
            for(int j=1;j<=8;j++)
            {
                System.out.printf("%3d",i*j);//+i*j+" ");
            }
            System.out.println("\n");
        }
    }     
 }
